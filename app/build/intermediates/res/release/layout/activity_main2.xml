<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    >

    <WebView android:id="@+id/webview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:src="@drawable/gur"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bt_fire"
        android:src="@drawable/fire"
        android:layout_below="@+id/bt"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="52dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bt_patient"
        android:src="@drawable/patient"
        android:layout_below="@+id/bt_fire"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/bt_fire" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn_ambulance"
        android:src="@drawable/ambulance"
        android:layout_below="@+id/bt_patient"
        android:layout_alignStart="@+id/bt_fire" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn_hospital"
        android:src="@drawable/hospital"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/bt"
        android:layout_alignStart="@+id/bt_fire" />


    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="核一廠EPZ3公里"
        android:id="@+id/check_epz3"
        android:layout_below="@+id/bt_patient"
        android:drawableLeft="@drawable/lay"
        android:layout_alignParentStart="true"
        android:checked="false" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="核一廠EPZ5公里"
        android:id="@+id/check_epz5"
        android:drawableLeft="@drawable/lay"
        android:layout_below="@+id/check_epz3"
        android:layout_alignParentStart="true"
        android:checked="false" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="核一廠EPZ8公里"
        android:id="@+id/check_epz8_1"
        android:drawableLeft="@drawable/lay"
        android:checked="false"
        android:layout_below="@+id/check_epz5"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="核一廠EPZ8公里(村莊)"
        android:id="@+id/check_epz8_2"
        android:drawableLeft="@drawable/lay"
        android:checked="false"
        android:layout_below="@+id/check_epz8_1"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="核一廠EPZ16公里"
        android:id="@+id/check_epz16_1"
        android:drawableLeft="@drawable/lay"
        android:checked="false"
        android:layout_below="@+id/check_epz8_2"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="核一廠EPZ16公里(村莊)"
        android:id="@+id/check_epz16_2"
        android:drawableLeft="@drawable/lay"
        android:checked="false"
        android:layout_below="@+id/check_epz16_1"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn_epz"
        android:layout_above="@+id/check_epz3"
        android:layout_toEndOf="@+id/bt"
        android:src="@drawable/c18d3427" />

</RelativeLayout><!-- From: file:/D:/NetBeansProjects/MyApplication/app/src/main/res/layout/activity_main2.xml -->